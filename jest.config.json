{
  "testEnvironment": "node",
  "rootDir": "./",
  "collectCoverage": false,
  "coverageDirectory": "coverage",
  "coveragePathIgnorePatterns": [],
  "collectCoverageFrom": [
    "./src/**/*.ts"
  ],
  "modulePathIgnorePatterns": [
    "./.git",
    "./node_modules"
  ],
  "transform": {
    "^.+\\.ts?$": "ts-jest"
  },
  "moduleFileExtensions": [
    "ts",
    "js",
    "json"
  ],
  "globals": {
    "ts-jest": {
      "tsconfig": "./tsconfig.jest.json"
    }
  }
}