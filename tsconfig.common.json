{
  "compilerOptions": {
    "baseUrl": ".",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "noErrorTruncation": true,
    "noEmit": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "outDir": "./lib",
    "strict": true,
    "target": "ES3"
  }
}
